{
  "name": "dm-pos-retail",
  "private": true,
  "version": "1.6.01",
  "author": "dmpos.dev@gmail.com",
  "description": "A desktop application for managing sales, inventory, and customers in a retail environment.",
  "main": "electron/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k \"vite\" \"electron .\"",
    "build": "vite build",
    "dist": "vite build && cross-env NODE_ENV=production electron-builder",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "release": "npm run build && npx electron-builder --publish always"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@zxing/library": "^0.21.3",
    "animate.css": "^4.1.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-barcode": "^1.6.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-number-format": "^5.4.4",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "sweetalert2": "^11.22.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.3"
  },
  "build": {
    "appId": "com.piccacode.dm-pos-solution",
    "productName": "DM POS",
    "publish": {
      "provider": "github",
      "owner": "DxMaurine",
      "repo": "dm-post-react",
      "private": false
      
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "extraResources": [
      {
        "from": "pos-backend/",
        "to": "pos-backend/",
        "filter": [
          "**/*",
          "!node_modules/**"
        ]
      }
    ],
    "asarUnpack": [
      "electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "pos-backend/public/favicon.ico",
      "uninstallerIcon": "pos-backend/public/favicon.ico"
    }
  }
}
